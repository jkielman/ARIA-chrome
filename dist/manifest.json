{
    "name": "Receptor",
    "version": "1.0",
    "manifest_version": 2,
    "author": "Jeff Kielman",
    "offline_enabled": true,
    "description": "Analyzes your webpage for A11y errors",
    "icons": {
     "16": "icons/icon16.png",
     "48": "icons/icon48.png",
     "128": "icons/icon128.png"
  },

  "permissions": ["tabs", "<all_urls>"],
    "content_scripts" : [
    {
      "matches": [ "<all_urls>" ],
      "js": ["index.js"]
    }
]
}


